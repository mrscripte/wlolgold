-- LP
lpSection:NewButton("Die", "Only Works On Games Where You Can Die", function()
    game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)
lpSection:NewTextBox("WalkSpeed", "", function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)
lpSection:NewTextBox("Jump Power", "", function(value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
end)
lpSection:NewTextBox("FOV","MAX FOV 120", function(txt) game.Workspace.CurrentCamera.FieldOfView = txt
end)
lpSection:NewToggle("Sprint", "", function(value)
    if value then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 24
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)
lpSection:NewButton("Normal WalkSpeed", "", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)
